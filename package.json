{
  "name": "instabot.js",
  "description": "An attempt to create instabot as gui app",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "start": "electron main",
    "start:prod": "NODE_ENV=production electron main",
    "build": "NODE_ENV=production poi build --generate-stats dist/stats.json",
    "test": "jest --colors",
    "lint": "eslint src/**/*.js"
  },
  "engines": {
    "node": "8.9.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "electron": "^10.1.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "given2": "^2.1.7",
    "jest": "^26.4.2",
    "mock-fs": "^4.13.0",
    "node-sass": "^4.14.1",
    "poi": "^12.10.2",
    "poi-preset-babel-minify": "^1.0.3",
    "sass-loader": "^10.0.2"
  },
  "dependencies": {
    "agent-base": "^6.0.1",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "formik": "^2.1.5",
    "fs-extra": "^9.0.1",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "instabot": "^0.1.3",
    "instagram-private-api": "^1.41.0",
    "ionicons": "^5.1.2",
    "lodash": "^4.17.21",
    "object-to-formdata": "^4.1.0",
    "promise.prototype.finally": "^3.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-localstorage": "^0.4.1",
    "socks-proxy-agent": "^5.0.0",
    "tough-cookie": "^4.0.0",
    "yup": "^0.29.3"
  },
  "externals": [
    "fs-extra",
    "request-promise-native",
    "tough-cookie"
  ],
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "given": true,
      "moment": true
    },
    "rules": {
      "no-debugger": [
        0
      ],
      "global-require": [
        0
      ],
      "react/prop-types": [
        2,
        {
          "ignore": [
            "children"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "jsx-a11y/click-events-have-key-events": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "jsx-a11y/alt-text": [
        0
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "support/webpack.config.js"
        }
      }
    },
    "overrides": [
      {
        "files": [
          "**/*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./support/jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true,
          "targets": {
            "node": "8.9.3"
          }
        }
      ]
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
